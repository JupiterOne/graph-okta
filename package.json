{
  "name": "@jupiterone/graph-okta",
  "version": "3.0.0",
  "description": "A JupiterOne Integration for https://www.okta.com",
  "license": "MPL-2.0",
  "repository": "https://github.com/jupiterone/graph-okta",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0 <19.x"
  },
  "scripts": {
    "start": "j1-integration collect",
    "graph": "j1-integration visualize",
    "graph:types": "j1-integration visualize-types",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc",
    "test": "jest",
    "test:env": "LOAD_ENV=1 yarn test",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "build": "tsc -p tsconfig.dist.json --declaration && cp README.md dist/README.md",
    "prepush": "yarn lint && yarn type-check && jest --changedSince main",
    "postversion": "cp package.json ./dist/package.json"
  },
  "dependencies": {
    "@okta/okta-sdk-nodejs": "^6.4.0",
    "lodash.startcase": "^4.4.0",
    "p-map": "^4",
    "promise-retry": "^1.1.1",
    "uuid": "^9.0.0",
    "@types/node": "^18"
  },
  "peerDependencies": {
    "@jupiterone/integration-sdk-core": "^9.4.0"
  },
  "devDependencies": {
    "auto": "^10.46.0",
    "@jupiterone/integration-sdk-core": "^9.4.0",
    "@jupiterone/integration-sdk-dev-tools": "^9.4.0",
    "@jupiterone/integration-sdk-testing": "^9.4.0",
    "@types/node-fetch": "^2.6.3",
    "@types/uuid": "^8.3.4",
    "typescript": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "ts-jest": "^27.1.5"
  },
  "auto": {
    "plugins": [
      [
        "npm",
        {
          "setRcToken": false,
          "publishFolder": "./dist"
        }
      ],
      "released"
    ],
    "onlyPublishWithReleaseLabel": true,
    "shipit": {
      "prerelease": false
    }
  }
}
